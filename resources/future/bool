:: nando a b c =
  (a ~ falseo /\ b ~ falseo /\ c ~ trueo ) \/
  (a ~ falseo /\ b ~ trueo  /\ c ~ trueo ) \/
  (a ~ trueo  /\ b ~ falseo /\ c ~ trueo ) \/
  (a ~ trueo  /\ b ~ trueo  /\ c ~ falseo)

:: noto a na =
  {nando a a na}

:: oro a b c =
  [aa bb:
    {nando a a aa} /\ {nando b b bb} /\ {nando aa bb c}]

:: ando a b c =
  [ab:
    {nando a b ab} /\ {nando ab ab c}]

